    "@types/swagger-ui-express": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.13.0",
    "supertest": "^6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
